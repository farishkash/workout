<div class="container">
<%=form_for [@tracker, @cardio_exercise] do |f| %>
  <%=f.label :cardio_exercise_name, "Exercise Name" %>
  <%=f.text_field :cardio_exercise_name, placeholder: "Enter Here" %>

  <%=f.label :time_minutes, "Length in Minutes" %>
  <%=f.text_field :time_minutes %>

  <%=f.label :calories_burned, "Calories Burned" %>
  <%=f.text_field :calories_burned %>

  <%=f.submit "Save", class:"btn btn-primary spaceleft" %> <%= link_to 'Cancel', tracker_path(@tracker), class:"btn btn-danger hideme spaceleft", remote: true %>

  <% end %>
</div>